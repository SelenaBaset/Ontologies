human:Role
	a rdfs:Class;
	rdfs:label "person role"@en;
	rdfs:comment """A role a person has as a social entity."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.
#	rdfs:subClassOf [a owl:Restriction; owl:onProperty agent:roleOf; owl:allValuesFrom human:Person].

human:Occupation
	a rdfs:Class;
	rdfs:label "occupation"@en;
	rdfs:comment """Action serving as a person's regular source of livelihood."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf event:Action.

human:OccupationalRole
	a rdfs:Class;
	rdfs:label "occupational role"@en;
	rdfs:comment """Role of a person in an occupation."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Role.

human:OccupationalState
	a rdfs:Class;
	rdfs:label "occupational state"@en;
	rdfs:comment """State of a person concerning an occupation."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf event:State;
	owl:oneOf (human:Working human:Retired human:OnSickLeave human:Unemployed).

human:Working
	a rdfs:Class, human:OccupationalState;
	rdfs:label "working"@en;
	rdfs:comment """Occupational state of having an occupation."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:Retired
	a rdfs:Class, human:OccupationalState;
	rdfs:label "retired"@en;
	rdfs:comment """Occupational state of having stopped working at a certain age as regulated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en.

human:OnSickLeave
	a rdfs:Class, human:OccupationalState;
	rdfs:label """on sick leave"""@en;
	rdfs:comment """Occupational state of having stopped working due to a health disorder."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:Unemployed
	a rdfs:Class, human:OccupationalState;
	rdfs:label "unemployed"@en;
	rdfs:comment """Occupational state of not having an occupation."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:Floruit # to move to agent:
	a rdfs:Class;
	rdfs:label "floruit"@en;
	rdfs:comment """Period during which a person lived or a school or movement was most active or flourishing."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf event:Action.

#
# PROPERTIES
#

human:hasAgent # to be changed to agent:hasAgent
	a owl:ObjectProperty;
	rdfs:label "has person agent"@en;
	rdfs:comment """Relating an action to a person."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:domain human:Action; # to be changed to agent:Action
	rdfs:range human:Person;
	kb:subjectClassConstraint human:Action; # Knora requirement
	kb:objectClassConstraint human:Person. # Knora requirement

human:hasAgentStatement # Knora requirement
	a owl:ObjectProperty;
	rdfs:label "has person agent statement"@en;
	rdfs:comment """Relating an action to a person statement as reification of the relation between the person and the role."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:domain human:Action; # to be changed to agent:Action
	rdfs:range human:Person;
	kb:subjectClassConstraint human:Action; # Knora requirement
	kb:objectClassConstraint human:Person. # Knora requirement

human:hasRole
	a owl:ObjectProperty;
	rdfs:label "person has role"@en;
	rdfs:comment """Relating a person to a role."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subPropertyOf kb:hasLinkTo;
	rdfs:domain human:Person;
	rdfs:range human:Role;
	kb:subjectClassConstraint human:Person; # Knora requirement
	kb:objectClassConstraint human:Role. # Knora requirement

human:hasRoleStatement # Knora requirement
	a owl:ObjectProperty;
	rdfs:label "has role statement"@en, "hat Rolle-Aussage"@de;
	rdfs:comment """Relating a person to a role statement as reification of the relation between the person and the role."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subPropertyOf kb:hasLinkToValue;
	kb:subjectClassConstraint human:Person; # Knora requirement
	kb:objectClassConstraint kb:LinkValue. # Knora requirement

human:hasBiologicalGender
	a owl:ObjectProperty;
	rdfs:label "has biological gender"@en;
	rdfs:comment """Relating a human to a biological gender."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:domain human:Human;
	rdfs:range human:BiologicalGender;
	kb:subjectClassConstraint human:Human; # Knora requirement
	kb:objectClassConstraint human:BiologicalGender. # Knora requirement

# human:begins

# human:beginDate

human:hasBirthDateTime
	a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:label "has birth date time"@en;
	rdfs:comment """Relating a human to a snapshot event's date and time of birth."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
#	owl:propertyChainAxiom (organism:lives organism:begins);
#	rdfs:subPropertyOf event:hasOriginationDateTime;
	rdfs:domain human:Human;
	rdfs:range xsd:dateTime;
	kb:subjectClassConstraint human:Human; # Knora requirement
	kb:objectDatatypeConstraint xsd:dateTime. # Knora requirement

human:hasBirthDate
	a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:label "has birth date"@en;
	rdfs:comment """Relating a human to a date of birth."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
#	owl:propertyChainAxiom (organism:lives organism:beginDate);
#	rdfs:subPropertyOf event:hasOriginationDate;
	rdfs:domain human:Human;
	rdfs:range xsd:date;
	kb:subjectClassConstraint human:Human; # Knora requirement
	kb:objectDatatypeConstraint xsd:date. # Knora requirement

# human:hasBirthTime

# human:ends

# human:endDate

human:hasDeathDateTime
	a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:label "has death date time"@en;
	rdfs:comment """Specifying a snapshot event's date and time an organism died on."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
#	owl:propertyChainAxiom (organism:lives organism:ends);
#	rdfs:subPropertyOf event:hasVanishingDateTime;
	rdfs:domain human:Human;
	rdfs:range xsd:dateTime;
	kb:subjectClassConstraint human:Human; # Knora requirement
	kb:objectDatatypeConstraint xsd:dateTime. # Knora requirement

human:hasDeathDate
	a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:label "has death date"@en;
	rdfs:comment """Specifying a snapshot event's date an organism died on."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
#	owl:propertyChainAxiom (organism:lives organism:endDate);
	rdfs:domain human:Human;
	rdfs:range xsd:date;
	kb:subjectClassConstraint human:Human; # Knora requirement
	kb:objectDatatypeConstraint xsd:date. # Knora requirement

# human:hasDeathTime

human:hasNameObject
	a owl:ObjectProperty;
	rdfs:label "has person name object";
	rdfs:comment """Relating a person to a name object."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subPropertyOf text:hasNameObject;
	rdfs:domain human:Person;
	rdfs:range human:NameObject;
	kb:subjectClassConstraint human:Person; # Knora requirement
	kb:objectClassConstraint human:NameObject. # Knora requirement
#	sg:guiOrder "1"^^xsd:integer; # Knora requirement
#	sg:guiElement sg:SimpleText; # Knora requirement
#	sg:guiAttribute "size=32", "maxlength=32". # Knora requirement

human:hasFamilyNameObject
	a owl:ObjectProperty;
	rdfs:label "has family name object";
	rdfs:comment """Relating a person to a family name object."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subPropertyOf text:hasNameObject;
	rdfs:domain human:Person;
	rdfs:range human:FamilyNameObject;
	kb:subjectClassConstraint human:Person; # Knora requirement
	kb:objectClassConstraint human:FamilyNameObject. # Knora requirement
#	sg:guiOrder "1"^^xsd:integer;
#	sg:guiElement sg:SimpleText;
#	sg:guiAttribute "size=32", "maxlength=32".

human:hasFamilyName
	a owl:DatatypeProperty;
	rdfs:label "has family name";
	rdfs:comment """Relating a person to a family name."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subPropertyOf foaf:familyName, text:hasName;
	owl:propertyChainAxiom (human:hasFamilyNameObject kb:valueHasString);
	rdfs:domain human:Person;
