<http://www.nie-ine.ch/ontology/human>
	a owl:Ontology;
	dct:title "An ontology about human"@en;
	dct:description """Formal description of a human, general subclasses, related classes and properties."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
#	owl:versionInfo "Version x - yyyy-mm-dd"@en;	# OR other format, datatype
	dct:creator "Hans Cools, software developer, NIE, Basel University, Switzerland"@en;
	dct:conformsTo "OWL Full"@en.

# general:
	# DB: mysql elements
		# KR: Raeber
		# PL: Lombardus
	# TBS: to be specified
	# SCO: rdfs:subClassOf
	# SPO: rdfs:subPropertyOf

#
# CLASSES
#

human:Human
	a rdfs:Class, unicore:Taxon;
	rdfs:label "human"@en, "Mensch"@de, "homme"@fr;
	rdfs:comment """Member of the subspecies Homo sapiens sapiens."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """A human lives but is not necessarily born; one can speak about a human embryo or foetus.""";
	unicore:scientificName """Homo sapiens sapiens"""@en;
	unicore:rank unicore:Subspecies;
	rdfs:subClassOf foaf:Agent, kb:Resource, <http://purl.uniprot.org/taxonomy/9606>. # species Homo sapiens; agent:Agent

human:Person # DB PL: RCS_AUTHORS
	a rdfs:Class;
	rdfs:label "person"@en;
	rdfs:comment """A real living born human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """A foaf:Person class member is born, but can be alive or dead, real or imaginary. In this more restricted meaning a person can do things during life and can be talked about after dead, having done things during life. An imaginary person is a thing that can have every conceivable property, very unlike a real person."""@en;
	rdfs:subClassOf human:Human, foaf:Person, frbroo:F10_Person, kb:Resource, [
		a owl:Restriction; owl:onProperty human:hasBiologicalGender; owl:someValuesFrom human:BiologicalGender], [
		a owl:Restriction; owl:onProperty human:hasBiologicalGender; owl:cardinality "1"^^xsd:nonNegativeInteger], [ # Knora requirement
		a owl:Restriction; owl:onProperty human:hasNameObject; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # Knora requirement
		a owl:Restriction; owl:onProperty human:hasFamilyNameObject; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # Knora requirement
		a owl:Restriction; owl:onProperty human:hasGivenNameObject; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # Knora requirement
		a owl:Restriction; owl:onProperty human:hasDescriptionObject; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # Knora requirement
		a owl:Restriction; owl:onProperty human:hasRole; owl:someValuesFrom human:Role], [
		a owl:Restriction; owl:onProperty human:hasRole; owl:minCardinality "1"^^xsd:nonNegativeInteger], [ # Knora requirement, but should be "0"
		a owl:Restriction; owl:onProperty human:hasRoleStatement; owl:minCardinality "1"^^xsd:nonNegativeInteger]. # Knora requirement

human:NameObject # part of DB PL: RCS_AUTHORS - NAME
	a rdfs:Class;
	rdfs:label "person name object";
	rdfs:comment "Name of a person, as an object.";
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Enabling speaking about a name as object, not as literal."""@en;
	skos:note """Further restriction given along specific law; e.g. can be changed."""@en;
	rdfs:subClassOf text:NameObject. # SCO text:TextObject SCO kb:TextValue

human:FamilyNameObject
	a rdfs:Class;
	rdfs:label "family name object";
	rdfs:comment "Name of a person, as an object, from one or both parents.";
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Enabling speaking about a family name as object, not as literal."""@en;
	skos:note """Further restriction given along specific law; e.g. can be changed."""@en;
	rdfs:subClassOf text:NameObject. # SCO text:TextObject SCO kb:TextValue

human:GivenNameObject # enables speaking about a given name as object, not as literal
	a rdfs:Class;
	rdfs:label "given name object";
	rdfs:comment "Name, as an object, given to a person, usually by one or both parents.";
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Enabling speaking about a given name as object, not as literal."""@en;
	skos:note """Further restriction given along specific law; e.g. can be changed."""@en;
	rdfs:subClassOf text:NameObject. # SCO text:TextObject SCO kb:TextValue

human:DescriptionObject # part of DB PL: RCS_AUTHORS - NOTES 
	a rdfs:Class;
	rdfs:label "person description object";
	rdfs:comment "Description of a person, as an object.";
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Enabling speaking about a person description as object, not as literal."""@en;	
	rdfs:subClassOf text:DescriptionObject. # SCO text:TextObject SCO kb:TextValue

human:Life
	a rdfs:Class;
	rdfs:label "human life"@en;
	rdfs:comment """Life of a human from birth until death."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Considered as an action wherein a person is an agent."""@en;
#	skos:note """No satisfying biological description of onset of a human life."""@en;
	rdfs:subClassOf event:Action, event:Existence, [
			a owl:Restriction; owl:onProperty human:hasAgent; owl:someValuesFrom human:Person], [ # to change to agent:hasAgent
			a owl:Restriction; owl:onProperty human:hasAgent; owl:cardinality "1"^^xsd:nonNegativeInteger]. # Knora requirement

human:BiologicalGender
	a rdfs:Class;
	rdfs:label "human biological gender"@en;
	rdfs:comment """Biological gender of a human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	owl:oneOf (human:FemaleGender human:IntersexualGender human:MaleGender).

human:FemaleGender
	a rdfs:Class, human:BiologicalGender;
	rdfs:label "humanly female"@en;
	rdfs:comment """Being of a human female gender."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:IntersexualGender
	a rdfs:Class, human:BiologicalGender;
	rdfs:label """humanly intersexual"""@en;
	rdfs:comment """Being of a gender that is neither male or female, with biological characteristics of both."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:MaleGender
	a rdfs:Class, human:BiologicalGender;
	rdfs:label "humanly male"@en;
	rdfs:comment """Being of a human male gender."""@en;
#	skos:note """In adulthood producing spermatozoa for fertilizing female ova."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:Female
	a rdfs:Class;
	rdfs:label "human female"@en;
	rdfs:comment """Human of a female biological gender."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Human, [
		a owl:Restriction; owl:onProperty human:hasBiologicalGender; owl:allValuesFrom human:FemaleGender].

human:Intersexual
	a rdfs:Class;
	rdfs:label "human intersexual"@en;
	rdfs:comment """Human of the intersexual biological gender."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Human, [
		a owl:Restriction; owl:onProperty human:hasBiologicalGender; owl:allValuesFrom human:IntersexualGender].

human:Male
	a rdfs:Class;
	rdfs:label "human male"@en;
	rdfs:comment """Human of the male biological gender."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Human, [
		a owl:Restriction; owl:onProperty human:hasBiologicalGender; owl:allValuesFrom human:MaleGender].

human:ImmatureHuman
	a rdfs:Class;
	rdfs:label """immature human"""@en;
	rdfs:comment """Biologically not full-grown person."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Person. # organism:ImmatureOrganism

human:BiologicalAdult
	a rdfs:Class;
	rdfs:label "biologically adult human"@en;
	rdfs:comment """Person having attained reproductive ability or evidencing secondary sex characteristics."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Person. # , [ # organism:MatureOrganism
#		a owl:Restriction; owl:onProperty organism:hasChild; owl:allValuesFrom human:Person].

human:LegalAdult
	a rdfs:Class;
	rdfs:label "legally adult human"@en;
	rdfs:comment """Person being adult as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en;
	rdfs:subClassOf human:Person.

human:Woman
	a rdfs:Class;
	rdfs:label "woman"@en;
	rdfs:comment """Adult human female."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Female, human:BiologicalAdult.

human:Women
	a rdfs:Class;
	rdfs:label "women"@en;
	rdfs:comment """Group of which a member is a woman."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	owl:disjointWith human:Men;
	rdfs:subClassOf foaf:Group, [
		a owl:Restriction; owl:onProperty foaf:member; owl:someValuesFrom human:Woman].
	
human:PregnantWoman
	a rdfs:Class;
	rdfs:label "pregnant woman"@en;
	rdfs:comment """Woman with offspring developing in her body."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Woman. # organism:PregnantOrganism.

human:Man
	a rdfs:Class;
	rdfs:label "man"@en;
	rdfs:comment """Adult human male."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:Male, human:BiologicalAdult.

human:Men
	a rdfs:Class;
	rdfs:label "men"@en;
	rdfs:comment """Group of which a member is a man."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf foaf:Group, [
		a owl:Restriction; owl:onProperty foaf:member; owl:someValuesFrom human:Man].

human:BiologicalParent
	a rdfs:Class;
	rdfs:label "biological human parent"@en;
	rdfs:comment """First grade living ascendant of a human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:BiologicalAdult. # organism:Parent.

human:BiologicalMother
	a rdfs:Class;
	rdfs:label "biological mother"@en;
	rdfs:comment """Female biological parent of a human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:BiologicalParent, human:Woman.

human:BiologicalFather
	a rdfs:Class;
	rdfs:label "biological father"@en;
	rdfs:comment """Male biological parent of a human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	rdfs:subClassOf human:BiologicalParent, human:Man.

human:LegalParent
	a rdfs:Class;
	rdfs:label "legal human parent"@en;
	rdfs:comment """Parent of a human as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en.

human:LegalMother
	a rdfs:Class;
	rdfs:label "legal mother"@en;
	rdfs:comment """Female parent of a human as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en;
	rdfs:subClassOf human:Woman.

human:LegalFather
	a rdfs:Class;
	rdfs:label "legal father"@en;
	rdfs:comment """Male parent of a human as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en;
	rdfs:subClassOf human:Man.

human:Wife
	a rdfs:Class;
	rdfs:label "wife"@en;
	rdfs:comment """Female partner in a marriage as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en;
	rdfs:subClassOf human:Woman.

human:Husband
	a rdfs:Class;
	rdfs:label "husband"@en;
	rdfs:comment """Male partner in a marriage as stated by law."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>;
	skos:note """Further restriction given along specific law."""@en;
	rdfs:subClassOf human:Man.

human:Partner
	a rdfs:Class;
	rdfs:label "partner"@en;
	rdfs:comment """Partner in a relationship."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

