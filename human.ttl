# human:LegalSister

# human:BiologicalCousin

# human:LegalCousin

human:BiologicalChild
	a rdfs:Class;
	rdfs:label "biological child"@en;
	skos:definition """First grade living descendant of a human."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:BiologicalChildren
	a rdfs:Class;
	rdfs:label "biological children"@en;
	skos:definition """Children of the same biological parent(s)."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

human:Offspring
	a rdfs:Class;
	rdfs:label "biological offspring"@en;
	skos:definition """Descendant(s) of the same biological parent(s)."""@en;
	rdfs:isDefinedBy <http://www.nie-ine.ch/ontology/human#>.

